    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "# Grafica 2\n",
    "x=np.arange(-10,10) # Rango de -10, 10\n",
    "y=A*(x**2)+ B*x + C\n",
    "plt.plot(x,y)\n",
    "plt.title(f\"Grafica {A} x^2 + {B} x + {C}\")\n",
    "plt.xlabel(\"Abscisa\")\n",
    "plt.ylabel(\"Ordenada\")\n",
    "plt.show()"
   ]
