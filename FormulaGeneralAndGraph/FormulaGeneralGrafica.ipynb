   "source": [
    "x=-10\n",
    "xa1=10\n",
    "while x < xa1:\n",
    "    if x != x1 and x != x2:\n",
    "        y = A*x**2+B*x+C\n",
    "        plt.plot(x,y,'.r')\n",
    "        x=x+.5\n",
    "    else:\n",
    "        y = A*x**2+B*x+C\n",
    "        plt.plot(x,y,'.b')\n",
    "        print(f\"Se grafico la raiz ( {x} , {y} )\")\n",
    "        x=x+.5\n",
    "plt.title(\"Grafica usando while\")\n",
    "plt.xlabel(\"Eje X\")\n",
    "plt.ylabel(\"Eje Y\")\n",
    "plt.show()"
